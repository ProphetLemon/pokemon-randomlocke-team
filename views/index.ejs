<body>
  <div class="container mt-5">
    <div class="container mb-5">
      <div class="rounded mb-2 cuadrado inline">
        <a href="/tabletypes"><b>Tabla de tipos</b></a>
      </div>
      <div class="rounded mb-2 inline ms-2 cuadrado">
        <a href="/team"><b>Creador de equipo</b></a>
      </div>
    </div>
    <input id="nombrePokemon" list="listaPokemon" type="text" />
    <input type="button" onclick="buscar()" value="Buscar" />
    <datalist id="listaPokemon">

    </datalist>
    <div id="imagen" class="container">
      <div class="rounded mt-2 cuadrado">
        <b>Escribe el nombre de un Pokémon</b>
      </div>
    </div>

    <div id="creditos" class="rounded mt-5 cuadrado">
      <i>Página hecha por <a href="https://twitter.com/profetalimon" target="_blank">Diego Vidal del
          Rosal</a></i>
    </div>
  </div>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
